persistent adopters : Array<Int32>;


@init
function constructor(owner : Address)

    use adopters;
    var myArray = Array<Int32>(5);
    myArray[0] = 0;
    myArray[1] = 0;
    myArray[2] = 0;
    myArray[3] = 0;
    myArray[4] = 0;
    adopters.set(myArray);

endfunction
